{
  "name": "radaller-core",
  "version": "0.2.1",
  "description": "Cms engine, that allows to store content as yaml.",
  "main": "index",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist",
    "src",
    "*.md",
    "*.js"
  ],
  "scripts": {
    "test": "jest",
    "build": "babel src -d dist --ignore '**/*.test.js'"
  },
  "jest": {
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radaller/radaller-core.git"
  },
  "keywords": [
    "cms",
    "git",
    "file",
    "yaml",
    "react"
  ],
  "dependencies": {
    "ajv": "^5.2.2",
    "axios": "^0.16.2",
    "fs-extra": "^4.0.1",
    "github-api": "^3.0.0",
    "js-base64": "^2.1.9",
    "js-yaml": "^3.9.1",
    "json-schema-ref-parser": "^3.2.0",
    "pluralize": "^6.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.18",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-runtime": "~6.18.0",
    "jest": "^20.0.4"
  },
  "bugs": {
    "url": "https://github.com/radaller/radaller-core/issues"
  },
  "homepage": "https://github.com/radaller/radaller-core#readme"
}
